      "source": [
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Load the data\n",
        "df = pd.read_csv('world-population-by-country-2020.csv')\n",
        "\n",
        "# Convert 'Population 2020' column to numeric\n",
        "df['Population 2020'] = pd.to_numeric(df['Population 2020'].str.replace(',', ''))\n",
        "\n",
        "# Sort countries by population\n",
        "sorted_population_df = df.sort_values(by='Population 2020', ascending=False)\n",
        "largest_population = sorted_population_df.head(7)\n",
        "\n",
        "# Calculate the total population of the largest 7 countries\n",
        "total_largest_population = largest_population['Population 2020'].sum()\n",
        "\n",
        "# Calculate the population of all remaining countries\n",
        "remaining_population = sorted_population_df['Population 2020'].sum() - total_largest_population\n",
        "\n",
        "# For populations\n",
        "populations_df = largest_population.copy()\n",
        "populations_df = populations_df.append({'Country (or dependency)': 'Others', 'Population 2020': remaining_population}, ignore_index=True)\n",
        "\n",
        "# For areas\n",
        "areas_df = largest_area.copy()\n",
        "areas_df = areas_df.append({'Country (or dependency)': 'Others', 'Land Area (KmÂ²)': remaining_area}, ignore_index=True)\n",
        "\n",
        "# Convert 'Land Area (KmÂ²)' column to numeric\n",
        "df['Land Area (KmÂ²)'] = pd.to_numeric(df['Land Area (KmÂ²)'].str.replace(',', ''))\n",
        "\n",
        "# Sort countries by land area\n",
        "sorted_area_df = df.sort_values(by='Land Area (KmÂ²)', ascending=False)\n",
        "largest_area = sorted_area_df.head(7)\n",
        "\n",
        "# Calculate the total land area of the largest 7 countries\n",
        "total_largest_area = largest_area['Land Area (KmÂ²)'].sum()\n",
        "\n",
        "# Calculate the land area of all remaining countries\n",
        "remaining_area = sorted_area_df['Land Area (KmÂ²)'].sum() - total_largest_area\n",
        "\n",
        "# Define a list of colors for the countries\n",
        "country_colors = {\n",
        "    'China': 'gold',\n",
        "    'India': 'lightcoral',\n",
        "    'United States': 'lightskyblue',\n",
        "    'Indonesia': 'lightgreen',\n",
        "    'Pakistan': 'lightpink',\n",
        "    'Brazil': 'lightsalmon',\n",
        "    'Nigeria': 'lightseagreen',\n",
        "    'Russia' : \"Red\",\n",
        "    'Canada'  :'pink' ,\n",
        "    'Australia':'Blue',\n",
        "\n",
        "}\n",
        "\n",
        "# Create figure and subplots\n",
        "fig, axs = plt.subplots(1, 2, figsize=(16, 8))\n",
        "\n",
        "# Pie chart of population values\n",
        "axs[0].pie(\n",
        "    populations_df['Population 2020'],\n",
        "    labels=populations_df['Country (or dependency)'],\n",
        "    autopct='%1.1f%%',\n",
        "    startangle=140,\n",
        "    colors=[country_colors.get(country, 'gray') for country in populations_df['Country (or dependency)']]\n",
        ")\n",
        "axs[0].set_title('Largest populations')\n",
        "\n",
        "# Pie chart of land area values\n",
        "axs[1].pie(\n",
        "    areas_df['Land Area (KmÂ²)'],\n",
        "    labels=areas_df['Country (or dependency)'],\n",
        "    autopct='%1.1f%%',\n",
        "    startangle=140,\n",
        "    colors=[country_colors.get(country, 'gray') for country in areas_df['Country (or dependency)']]\n",
        ")\n",
        "axs[1].set_title('Largest land areas')\n",
        "\n",
        "# Adjust spacing between subplots\n",
        "plt.subplots_adjust(wspace=0.2)\n",
        "\n",
        "# Show the plot\n",
        "plt.show()\n"
      ]
    }
  ]
